<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<window id="2005">
    <onload>ClearProperty(ViewType,Home)</onload>
    <controls>

        <control type="group" id="1">
            <visible>!String.IsEmpty(Skin.String(censor))</visible>
            <control type="image" id="16">
                <width>1870</width>
                <height>1030</height>
                <top>25</top>
                <left>25</left>
                <texture flipy="true">common/black-square.png</texture>
            </control>
            <control type="label" id="15">
                <width>auto</width>
                <label>$INFO[Skin.String(censor)]</label>
                <height>1080</height>
                <align>center</align>
                <aligny>center</aligny>
                <textcolor>panel_fg_100</textcolor>
            </control>
        </control>

        <control type="group" id="0">
            <left>0</left>
            <include>Animation_FadeIn</include>
            <visible>Control.IsVisible(10) | Control.IsVisible(11) | Control.IsVisible(12)</visible>
            <control type="image">
                <width>1920</width>
                <height>260</height>
                <texture colordiffuse="panel_bg" flipy="false">common/white.png</texture>
            </control>
            <control type="grouplist">
                <left>view_pad</left>
                <top>view_pad</top>
                <orientation>vertical</orientation>
                <control type="label" id="10">
                    <description>row 1 label</description>
                    <width>auto</width>
                    <height>40</height>
                    <align>left</align>
                    <aligny>top</aligny>
                    <textcolor>panel_fg_100</textcolor>
                </control>
                <control type="label" id="11">
                    <description>row 2 label</description>
                    <height>40</height>
                    <width>auto</width>
                    <align>left</align>
                    <aligny>top</aligny>
                    <textcolor>panel_fg_100</textcolor>
                </control>
                <control type="label" id="12">
                    <description>row 3 label</description>
                    <width>auto</width>
                    <height>40</height>
                    <align>left</align>
                    <aligny>top</aligny>
                    <textcolor>panel_fg_100</textcolor>
                </control>
            </control>
        </control>

    </controls>

</window>
