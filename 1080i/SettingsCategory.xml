<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<window id="12">
    <defaultcontrol always="true">300</defaultcontrol>
    <include>Defs_Settings_OnLoad</include>
    <onunload condition="!Skin.HasSetting(startup.init)">Skin.SetBool(startup.init)</onunload>
    <controls>

        <include>Global_Background</include>
        <include>Topbar</include>
        <control type="list" id="300">
            <ondown>3</ondown>
            <include>Topbar_Menu_Layout</include>
            <include>Items_Settings</include>
        </control>
        
        <!-- GroupList -->
        <control type="group">
            <include>Dialog_Settings_Panel</include>
            <control type="group">
                <include>Object_AZ_Version</include>

                <control type="group">
                    <top>view_list_height</top>
                    <!-- Right Group -->
                    <control type="grouplist" id="5">
                        <description>Control Area</description>
                        <left>450</left>
                        <right>0</right>
                        <top>0</top>
                        <height>702</height>
                        <itemgap>0</itemgap>
                        <onleft>3</onleft>
                        <include>Defs_Settings_OnRight</include>
                        <orientation>vertical</orientation>
                        <scrolltime>0</scrolltime>
                    </control>

                    <!-- Left Group -->
                    <control type="grouplist" id="3">
                        <description>Button Area</description>
                        <width>450</width>
                        <top>0</top>
                        <height>702</height>
                        <itemgap>0</itemgap>
                        <onright>5</onright>
                        <ondown>20</ondown>
                        <include>Defs_Settings_OnLeft</include>
                        <orientation>vertical</orientation>
                    </control>

                    <!-- Settings Change -->
                    <control type="button" id="20">
                        <width>450</width>
                        <bottom>0</bottom>
                        <onup>3</onup>
                        <onright>3</onright>
                        <onclick>SettingsLevelChange</onclick>
                        <texturefocus colordiffuse="main_fg_100">common/white.png</texturefocus>
                        <include>Defs_Settings_Button</include>
                    </control>


                    <!-- Lines -->
                    <include>Defs_Settings_Line</include>

                    <!-- Arrows -->
                    <include content="Defs_Settings_Arrows">
                        <param name="id" value="5" />
                    </include>
                </control>

               
            </control>
        </control>

        <control type="group">
            <include content="Dialog_Notification_Group">
                <param name="main_id" value="9901" />
                <param name="sub_id" value="6" />
                <param name="label" value="19033" />
            </include>
        </control>

        <control type="button" id="7">
            <description>Default Button</description>
            <include>Defs_Settings_Button</include>
        </control>

        <control type="radiobutton" id="8">
            <description>Default Radio Button</description>
            <include>Defs_Settings_Button</include>
        </control>

        <control type="spincontrolex" id="9">
            <description>Default Spin Control</description>
            <include>Defs_Settings_Button</include>
        </control>

        <control type="togglebutton" id="10">
            <width>450</width>
            <description>Default Category Button</description>
            <include>Defs_Settings_Button</include>
        </control>

        <control type="label" id="14">
            <textoffsetx>view_pad</textoffsetx>
            <font>font_button</font>
            <textcolor>main_fg_30</textcolor>
            <include>Defs_Settings_Button</include>
        </control>

    </controls>

</window>
