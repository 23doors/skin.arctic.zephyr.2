<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="View_51_Wall_Label">
        <control type="label">
            <left>15</left>
            <right>15</right>
            <font>font_tiny</font>
            <textcolor>main_fg_100</textcolor>
            <top>$PARAM[iconheight]</top>
            <bottom>30</bottom>
            <aligny>center</aligny>
            <align>center</align>
            <label>$INFO[ListItem.Label]</label>
        </control>
    </include>

    <include name="View_51_Wall_Landscape_Label">
        <control type="label">
            <left>15</left>
            <right>15</right>
            <font>font_tiny</font>
            <textcolor>main_fg_100</textcolor>
            <top>343.75</top>
            <height>20</height>
            <aligny>center</aligny>
            <align>left</align>
            <label>$INFO[ListItem.Label]</label>
        </control>
        <control type="label">
            <left>15</left>
            <right>15</right>
            <font>font_tiny</font>
            <textcolor>main_fg_100</textcolor>
            <top>343.75</top>
            <height>20</height>
            <aligny>center</aligny>
            <align>right</align>
            <label>$INFO[ListItem.Label2]</label>
        </control>
        <control type="group">
            <top>30</top>
            <visible>String.StartsWith(Container.FolderPath,plugin://plugin.video.youtube)</visible>
            <control type="label">
                <left>15</left>
                <right>15</right>
                <font>font_tiny</font>
                <textcolor>main_fg_70</textcolor>
                <top>343.75</top>
                <height>20</height>
                <aligny>center</aligny>
                <align>left</align>
                <label>$INFO[ListItem.Studio]</label>
            </control>
            <control type="label">
                <left>15</left>
                <right>15</right>
                <font>font_tiny</font>
                <textcolor>main_fg_70</textcolor>
                <top>343.75</top>
                <height>20</height>
                <aligny>center</aligny>
                <align>right</align>
                <label>$INFO[ListItem.Premiered]</label>
            </control>
        </control>
    </include>

    <include name="View_51_Wall_Itemlayout">
        <param name="icon" default="$VAR[Image_Poster]" />
        <param name="iconheight" default="345" />
        <param name="labelinclude" default="View_51_Wall_Label" />
        <param name="aspectratio" default="scale" />
        <definition>
            <control type="group">
                <height>$PARAM[iconheight]</height>
                <include>Object_Geometric</include>
                <include>Object_DefaultIcon</include>
                <include content="Object_Poster">
                    <param name="icon" value="$PARAM[icon]" />
                    <param name="aspectratio" value="$PARAM[aspectratio]" />
                </include>
                <include>View_WatchedOverlay</include>
                <control type="group">
                    <visible>Control.HasFocus($PARAM[id])</visible>
                    <include condition="$PARAM[selectbox]">View_Selectbox</include>
                </control>
            </control>
            <include content="$PARAM[labelinclude]">
                <param name="iconheight" value="$PARAM[iconheight]" />
            </include>
        </definition>
    </include>

    <include name="View_51_Wall_Container">
        <param name="itemlayout" default="View_51_Wall_Itemlayout" />
        <param name="itemwidth" default="244.28" />
        <param name="itemheight" default="412.5" />
        <param name="controltype" default="panel" />
        <param name="controllayout" default="View_Standard" />
        <param name="controlorientation" default="View_Vertical" />
        <param name="visible" default="true" />
        <param name="onfocus" default="" />
        <param name="includecontent" default="false" />
        <param name="onleft" default="" />
        <param name="onright" default="" />
        <param name="onback" default="" />
        <param name="onup" default="" />
        <param name="ondown" default="" />
        <param name="aspectratio" default="scale" />
        <definition>
            <control type="$PARAM[controltype]" id="$PARAM[id]">
                <include>$PARAM[controllayout]</include>
                <include>$PARAM[controlorientation]</include>
                <height>$PARAM[height]</height>
                <onfocus>$PARAM[onfocus]</onfocus>
                <onleft>$PARAM[onleft]</onleft>
                <onright>$PARAM[onright]</onright>
                <onback>$PARAM[onback]</onback>
                <onup>$PARAM[onup]</onup>
                <ondown>$PARAM[ondown]</ondown>
                <visible>$PARAM[visible]</visible>
                <itemlayout width="$PARAM[itemwidth]" height="$PARAM[itemheight]">
                    <include content="$PARAM[itemlayout]">
                        <param name="selectbox" value="false" />
                        <param name="icon" value="$PARAM[icon]" />
                        <param name="iconheight" value="$PARAM[iconheight]" />
                        <param name="labelinclude" value="$PARAM[labelinclude]" />
                        <param name="id" value="$PARAM[id]" />
                        <param name="aspectratio" value="$PARAM[aspectratio]" />
                    </include>
                </itemlayout>
                <focusedlayout width="$PARAM[itemwidth]" height="$PARAM[itemheight]">
                    <include content="$PARAM[itemlayout]">
                        <param name="selectbox" value="true" />
                        <param name="icon" value="$PARAM[icon]" />
                        <param name="iconheight" value="$PARAM[iconheight]" />
                        <param name="labelinclude" value="$PARAM[labelinclude]" />
                        <param name="id" value="$PARAM[id]" />
                        <param name="aspectratio" value="$PARAM[aspectratio]" />
                    </include>
                </focusedlayout>
                <include content="View_Include_Content" condition="$PARAM[includecontent]">
                    <param name="content" value="$PARAM[content]" />
                </include>
            </control>
        </definition>
    </include>

    <include name="View_51_Wall_Control">
        <param name="itemlayout" default="View_51_Wall_Itemlayout" />
        <param name="itemwidth" default="244.28" />
        <param name="itemheight" default="412.5" />
        <param name="controltype" default="panel" />
        <param name="controllayout" default="View_Standard" />
        <param name="controlorientation" default="View_Vertical" />
        <param name="include" default="" />
        <param name="includeid" default="" />
        <param name="visible" default="true" />
        <param name="onfocus" default="" />
        <param name="includecontent" default="false" />
        <param name="onleft" default="" />
        <param name="onright" default="" />
        <definition>
            <control type="group">
                <include content="View_Group">
                    <param name="infohide" value="$PARAM[infohide]" />
                    <param name="animation" value="$PARAM[animation]" />
                </include>
                <visible>Control.IsVisible($PARAM[id])</visible>
                <visible>$PARAM[visible]</visible>
                <include content="View_51_Wall_Container">
                    <param name="controltype" value="$PARAM[controltype]" />
                    <param name="id" value="$PARAM[id]" />
                    <param name="controllayout" value="$PARAM[controllayout]" />
                    <param name="controlorientation" value="$PARAM[controlorientation]" />
                    <param name="height" value="$PARAM[height]" />
                    <param name="onfocus" value="$PARAM[onfocus]" />
                    <param name="onleft" value="$PARAM[onleft]" />
                    <param name="onright" value="$PARAM[onright]" />
                    <param name="itemwidth" value="$PARAM[itemwidth]" />
                    <param name="itemheight" value="$PARAM[itemheight]" />
                    <param name="itemlayout" value="$PARAM[itemlayout]" />
                    <param name="icon" value="$PARAM[icon]" />
                    <param name="iconheight" value="$PARAM[iconheight]" />
                    <param name="labelinclude" value="$PARAM[labelinclude]" />
                    <param name="includecontent" value="$PARAM[includecontent]" />
                    <param name="content" value="$PARAM[content]" />
                </include>
                <include content="$PARAM[include]">
                    <param name="id" value="$PARAM[includeid]" />
                </include>
            </control>
        </definition>
    </include>

    <include name="View_51_Wall">
        <include content="View_51_Wall_Control">
            <param name="id" value="51" />
            <param name="height" value="825" />
        </include>
    </include>
    <include name="View_510_Wall_Square">
        <include content="View_51_Wall_Control">
            <param name="itemwidth" value="342" />
            <param name="id" value="510" />
            <param name="height" value="825" />
        </include>
    </include>
    <include name="View_511_Wall_Landscape">
        <include content="View_51_Wall_Control">
            <param name="itemwidth" value="570" />
            <param name="itemheight" value="423.75" />
            <param name="id" value="511" />
            <param name="height" value="847.5" />
            <param name="icon" value="$VAR[Image_Fanart]" />
            <param name="iconheight" value="333.75" />
            <param name="labelinclude" value="View_51_Wall_Landscape_Label" />
        </include>
    </include>
</includes>

