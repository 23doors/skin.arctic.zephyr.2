<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<window id="34">
    <defaultcontrol always="true">300</defaultcontrol>
    <include>Defs_Settings_OnLoad</include>
    <controls>

        <include>Global_Background</include>
        <include>Topbar</include>
        <control type="list" id="300">
            <ondown>4</ondown>
            <include>Topbar_Menu_Layout</include>
            <include>Items_Settings</include>
        </control>

        <!-- GroupList -->
        <control type="group">
            <include>Dialog_Settings_Panel</include>
            <control type="group">
                <include>Object_AZ_Version</include>

                <control type="group">
                    <top>view_list_height</top>
                    <height>800</height>

                    <!-- Left Group -->
                    <control type="radiobutton" id="4">
                        <top>0</top>
                        <include>Defs_Settings_OnLeft</include>
                        <onright>2</onright>
                        <width>450</width>
                        <label>20096</label>
                        <radioposx>395</radioposx>
                        <include>Defs_Settings_Button</include>
                    </control>

                    <!-- Right Group -->
                    <control type="panel" id="2">
                        <description>Control Area</description>
                        <left>450</left>
                        <right>0</right>
                        <top>0</top>
                        <height>702</height>
                        <onleft>4</onleft>
                        <include>Defs_Settings_OnRight</include>
                        <orientation>vertical</orientation>
                        <scrolltime>0</scrolltime>
                        <itemlayout height="80" width="1480">
                            <control type="label">
                                <right>view_pad</right>
                                <left>50</left>
                                <textcolor>main_fg_100</textcolor>
                                <info>ListItem.Label</info>
                            </control>
                            <control type="label">
                                <right>view_pad</right>
                                <left>50</left>
                                <align>right</align>
                                <font>font_tiny</font>
                                <textcolor>main_fg_100</textcolor>
                                <info>ListItem.Label2</info>
                            </control>
                        </itemlayout>

                        <focusedlayout height="80" width="1480">
                            <control type="group">
                                <control type="image">
                                    <width>100%</width>
                                    <texture border="5" colordiffuse="main_fg_100">common/white.png</texture>
                                </control>
                                <control type="label">
                                    <left>50</left>
                                    <right>view_pad</right>
                                    <textcolor>main_bg_100</textcolor>
                                    <info>ListItem.Label</info>
                                </control>
                                <control type="label">
                                    <right>view_pad</right>
                                    <left>50</left>
                                    <align>right</align>
                                    <font>font_tiny</font>
                                    <textcolor>main_bg_100</textcolor>
                                    <info>ListItem.Label2</info>
                                </control>
                            </control>
                        </focusedlayout>
                    </control>

                    <!-- Lines -->
                    <include>Defs_Settings_Line</include>

                    <!-- Arrows -->
                    <include content="Defs_Settings_Arrows">
                        <param name="id" value="5" />
                    </include>
                </control>
            </control>
        </control>

        <control type="group">
            <include content="Dialog_Notification_Group">
                <param name="main_id" value="9901" />
                <param name="sub_id" value="9902" />
                <param name="label" value="19033" />
                <param name="label2" value="31234" />
            </include>
        </control>
        <include>Topbar</include>


        
    </controls>

</window>
